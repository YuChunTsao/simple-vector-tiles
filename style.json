        "type": "symbol",
        "source": "tiles",
        "source-layer": "places",
        "minzoom": 0,
        "maxzoom": 24,
        "layout": {
          "text-field": "{name}",
          "text-size": {
            "stops": [
              [3, 12],
              [8, 22]
            ]
          }
        },
        "paint": {
          "text-color": "rgba(57, 57, 57, 1)",
          "text-halo-width": 1,
          "text-halo-blur": 1,
          "text-halo-color": "rgba(255, 255, 255, 1)"
